{
  "expo": {
    "name": "Suzuki Vehicle Evaluation",
    "slug": "suzuki",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.saqibmujeeb.app"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-task-manager",
      "expo-notifications",
      "expo-background-fetch",
      [
        "sentry-expo",
        {
          "organization": "echo-hy",
          "project": "react-native",
          "authToken": "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"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./app/assets/fonts/Suzuki-Bold.ttf",
            "./app/assets/fonts/Suzuki-Regular.ttf",
            "node_modules/@expo-google-fonts/poppins/Poppins_400Regular.ttf",
            "node_modules/@expo-google-fonts/poppins/Poppins_500Medium.ttf",
            "node_modules/@expo-google-fonts/poppins/Poppins_600SemiBold.ttf"
          ]
        }
      ],
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "5612f6c3-7e88-417b-8b09-6b0a44b5d108"
      }
    },
    "owner": "saqibmujeeb"
  }
}
